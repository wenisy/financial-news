{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/index.js"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "backend",
  "env": {
    "NODE_ENV": "production"
  }
}
